include "base.fbs";
include "fdr.fbs";

namespace bughive.policy;

enum RequestType : int32 {
    REQ_FDR_STATE_OPERATOR,
}

table Request {
    request:RequestType;
    state:bughive.fdr.State;
}

table ResponseFDROperator {
    response:bughive.Response (required);
    operator:int32 = -1;
}

